[
    // ==================== 基础编辑 ====================
    // 复制/剪切/粘贴 (macOS 标准)
    { "key": "cmd+c", "command": "editor.action.clipboardCopyAction" },
    { "key": "cmd+x", "command": "editor.action.clipboardCutAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "cmd+v", "command": "editor.action.clipboardPasteAction" },

    // 撤销/重做
    { "key": "cmd+z", "command": "undo" },
    { "key": "shift+cmd+z", "command": "redo" },

    // 删除行 (IDEA: Cmd+Backspace)
    { "key": "cmd+backspace", "command": "editor.action.deleteLines", "when": "textInputFocus && !editorReadonly" },

    // 复制行 (IDEA: Cmd+D)
    { "key": "cmd+d", "command": "editor.action.copyLinesDownAction", "when": "editorTextFocus && !editorReadonly && !editorHasSelection" },

    // 移动行 (IDEA: Shift+Alt+Up/Down)
    { "key": "shift+alt+up", "command": "editor.action.moveLinesUpAction", "when": "editorTextFocus && !editorReadonly" },
    { "key": "shift+alt+down", "command": "editor.action.moveLinesDownAction", "when": "editorTextFocus && !editorReadonly" },

    // 合并行 (IDEA: Ctrl+Shift+J)
    { "key": "shift+ctrl+j", "command": "editor.action.joinLines", "when": "editorTextFocus && !editorReadonly" },

    // 插入空行
    { "key": "shift+enter", "command": "editor.action.insertLineBefore", "when": "editorTextFocus && !editorReadonly" },
    { "key": "cmd+enter", "command": "lineBreakInsert", "when": "textInputFocus && !editorReadonly" },

    // ==================== 注释 ====================
    // 行注释 (IDEA: Cmd+/)
    { "key": "cmd+/", "command": "editor.action.commentLine", "when": "editorTextFocus && !editorReadonly" },

    // 块注释 (IDEA: Cmd+Alt+/ 或 Ctrl+Shift+/)
    { "key": "cmd+alt+/", "command": "editor.action.blockComment", "when": "editorTextFocus && !editorReadonly" },
    { "key": "shift+ctrl+/", "command": "editor.action.blockComment", "when": "editorTextFocus && !editorReadonly" },

    // ==================== 代码折叠 ====================
    { "key": "cmd+-", "command": "editor.fold", "when": "editorTextFocus && foldingEnabled" },
    { "key": "cmd+=", "command": "editor.unfold", "when": "editorTextFocus && foldingEnabled" },
    { "key": "shift+cmd+-", "command": "editor.foldAll", "when": "editorTextFocus && foldingEnabled" },
    { "key": "shift+cmd+=", "command": "editor.unfoldAll", "when": "editorTextFocus && foldingEnabled" },
    { "key": "cmd+alt+-", "command": "editor.foldRecursively", "when": "editorTextFocus && foldingEnabled" },
    { "key": "cmd+alt+=", "command": "editor.unfoldRecursively", "when": "editorTextFocus && foldingEnabled" },

    // ==================== 导航 ====================
    // 跳转到定义 (IDEA: Cmd+B 或 Cmd+Click)
    { "key": "cmd+b", "command": "editor.action.revealDefinition", "when": "editorTextFocus" },
    { "key": "cmd+down", "command": "editor.action.goToDeclaration", "when": "editorTextFocus" },
    { "key": "f4", "command": "editor.action.goToDeclaration", "when": "editorTextFocus" },

    // 跳转到实现 (IDEA: Cmd+Alt+B)
    { "key": "cmd+alt+b", "command": "editor.action.goToImplementation", "when": "editorHasImplementationProvider && editorTextFocus" },

    // 跳转到类型定义 (IDEA: Cmd+Shift+B)
    { "key": "shift+cmd+b", "command": "editor.action.goToTypeDefinition", "when": "editorTextFocus" },

    // 查找引用 (IDEA: Alt+F7)
    { "key": "alt+f7", "command": "references-view.findReferences", "when": "editorHasReferenceProvider" },
    { "key": "cmd+alt+f7", "command": "editor.action.referenceSearch.trigger", "when": "editorHasReferenceProvider && editorTextFocus" },

    // 调用层次 (IDEA: Ctrl+Alt+H)
    { "key": "ctrl+alt+h", "command": "references-view.showCallHierarchy", "when": "editorHasCallHierarchyProvider" },

    // 类型层次 (Java)
    { "key": "ctrl+h", "command": "java.action.showTypeHierarchy", "when": "editorLangId == java && javaLSReady && editorTextFocus" },

    // 前进/后退
    { "key": "cmd+o", "command": "workbench.action.navigateBack", "when": "canNavigateBack" },
    { "key": "cmd+i", "command": "workbench.action.navigateForward", "when": "canNavigateForward" },
    { "key": "cmd+alt+left", "command": "workbench.action.navigateBack", "when": "canNavigateBack" },
    { "key": "cmd+alt+right", "command": "workbench.action.navigateForward", "when": "canNavigateForward" },

    // 切换标签页
    { "key": "cmd+[", "command": "workbench.action.previousEditor" },
    { "key": "cmd+]", "command": "workbench.action.nextEditor" },

    // 最近编辑位置 (IDEA: Cmd+Shift+Backspace)
    { "key": "shift+cmd+backspace", "command": "workbench.action.navigateToLastEditLocation" },

    // 下一个/上一个错误 (IDEA: F2 / Shift+F2)
    { "key": "f2", "command": "editor.action.marker.next", "when": "editorFocus" },
    { "key": "shift+f2", "command": "editor.action.marker.prev", "when": "editorFocus" },

    // ==================== 搜索 ====================
    // 搜索文件 (IDEA: Cmd+Shift+O, VSCode 习惯 Cmd+P)
    { "key": "cmd+p", "command": "workbench.action.openRecent" },
    { "key": "shift+cmd+o", "command": "workbench.action.quickOpen" },

    // 最近工作的文件 (当前项目内)
    { "key": "cmd+e", "command": "workbench.action.quickOpen" },

    // 搜索符号 (IDEA: Cmd+O 搜索类, Cmd+Alt+O 搜索符号)
    {
        "key": "cmd+alt+o",
        "command": "workbench.action.gotoSymbol",
        "when": "editorTextFocus"
    },
    { "key": "cmd+f12", "command": "workbench.action.gotoSymbol", "when": "editorTextFocus" },

    // 跳转到行 (IDEA: Cmd+L)
    { "key": "cmd+l", "command": "workbench.action.gotoLine" },

    // 文件内查找/替换
    { "key": "cmd+f", "command": "actions.find" },
    { "key": "cmd+r", "command": "editor.action.startFindReplaceAction", "when": "editorFocus || editorIsOpen" },

    // 全局查找/替换 (IDEA: Cmd+Shift+F / Cmd+Shift+R)
    { "key": "shift+cmd+f", "command": "workbench.action.findInFiles" },
    { "key": "shift+cmd+r", "command": "workbench.action.replaceInFiles" },

    // 查找下一个/上一个
    { "key": "cmd+g", "command": "editor.action.nextMatchFindAction", "when": "editorFocus" },
    { "key": "shift+cmd+g", "command": "editor.action.previousMatchFindAction", "when": "editorFocus" },
    { "key": "enter", "command": "editor.action.nextMatchFindAction", "when": "editorFocus && findInputFocussed" },

    // 选中所有匹配 (IDEA: Ctrl+Cmd+G)
    { "key": "ctrl+cmd+g", "command": "editor.action.selectHighlights", "when": "editorFocus" },
    { "key": "ctrl+g", "command": "editor.action.addSelectionToNextFindMatch", "when": "editorFocus" },

    // ==================== 重构 ====================
    // 重命名 (IDEA: Shift+F6)
    // 重命名 (IDEA: Shift+F6)
    { "key": "shift+f6", "command": "editor.action.rename", "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly" },

    // 快速修复 (IDEA: Alt+Enter)
    { "key": "alt+enter", "command": "editor.action.quickFix", "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly" },

    // 格式化代码 (IDEA: Cmd+Alt+L)
    { "key": "cmd+alt+l", "command": "editor.action.formatDocument", "when": "editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly" },

    // 优化导入 (IDEA: Ctrl+Alt+O)
    { "key": "ctrl+alt+o", "command": "editor.action.organizeImports", "when": "editorTextFocus && !editorReadonly && supportedCodeAction =~ /(\\s|^)source\\.organizeImports\\b/" },

    // ==================== 代码补全 ====================
    { "key": "ctrl+space", "command": "editor.action.triggerSuggest", "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly" },
    { "key": "ctrl+p", "command": "editor.action.triggerParameterHints", "when": "editorHasSignatureHelpProvider && editorTextFocus" },
    { "key": "enter", "command": "acceptSelectedSuggestion", "when": "suggestWidgetVisible && textInputFocus" },
    { "key": "tab", "command": "acceptAlternativeSelectedSuggestion", "when": "suggestWidgetVisible && textInputFocus" },

    // 显示悬停信息 (IDEA: F1 或 Ctrl+J)
    { "key": "f1", "command": "editor.action.showHover", "when": "editorTextFocus" },
    { "key": "ctrl+j", "command": "editor.action.showHover", "when": "editorTextFocus" },

    // 预览定义 (IDEA: Alt+Space 或 Cmd+Y)
    { "key": "alt+space", "command": "editor.action.peekDefinition", "when": "editorTextFocus" },
    { "key": "cmd+y", "command": "editor.action.peekDefinition", "when": "editorTextFocus" },

    // ==================== 选择 ====================
    // 扩展/收缩选择 (IDEA: Alt+Up/Down)
    { "key": "alt+up", "command": "editor.action.smartSelect.grow", "when": "editorTextFocus" },
    { "key": "alt+down", "command": "editor.action.smartSelect.shrink", "when": "editorTextFocus" },

    // 列选择模式
    { "key": "shift+cmd+8", "command": "editor.action.toggleColumnSelection" },

    // ==================== 光标移动 ====================
    // 行首/行尾
    { "key": "cmd+left", "command": "cursorLineStart", "when": "textInputFocus" },
    { "key": "cmd+right", "command": "cursorLineEnd", "when": "textInputFocus" },
    { "key": "home", "command": "cursorLineStart", "when": "textInputFocus" },
    { "key": "end", "command": "cursorLineEnd", "when": "textInputFocus" },
    { "key": "shift+cmd+left", "command": "cursorLineStartSelect", "when": "textInputFocus" },
    { "key": "shift+cmd+right", "command": "cursorLineEndSelect", "when": "textInputFocus" },

    // 文件首/尾
    { "key": "cmd+home", "command": "cursorTop", "when": "textInputFocus" },
    { "key": "cmd+end", "command": "cursorBottom", "when": "textInputFocus" },

    // 单词跳转
    { "key": "alt+left", "command": "cursorWordLeft", "when": "editorTextFocus" },
    { "key": "alt+right", "command": "cursorWordRight", "when": "editorTextFocus && !inlineSuggestionVisible" },
    { "key": "shift+alt+left", "command": "cursorWordLeftSelect", "when": "editorTextFocus" },
    { "key": "shift+alt+right", "command": "cursorWordRightSelect", "when": "editorTextFocus" },

    // 删除单词
    { "key": "alt+backspace", "command": "deleteWordLeft", "when": "textInputFocus && !editorReadonly" },
    { "key": "alt+delete", "command": "deleteWordRight", "when": "textInputFocus && !editorReadonly" },

    // 翻页
    { "key": "pageup", "command": "cursorPageUp", "when": "textInputFocus" },
    { "key": "pagedown", "command": "cursorPageDown", "when": "textInputFocus" },

    // ==================== 运行/调试 ====================
    // 运行 (IDEA: Ctrl+R)
    { "key": "ctrl+r", "command": "workbench.action.debug.run", "when": "debuggersAvailable && !inDebugMode && !terminalFocus" },

    // 调试 (IDEA: Ctrl+D)
    { "key": "ctrl+d", "command": "workbench.action.debug.start", "when": "debuggersAvailable && !inDebugMode && !terminalFocus" },

    // 继续 (IDEA: Cmd+Alt+R 或 F9)
    { "key": "cmd+alt+r", "command": "workbench.action.debug.continue", "when": "inDebugMode" },
    { "key": "f9", "command": "workbench.action.debug.continue", "when": "inDebugMode" },

    // 单步 (IDEA: F7/F8/Shift+F8)
    { "key": "f7", "command": "workbench.action.debug.stepInto", "when": "debugState != 'inactive'" },
    { "key": "f8", "command": "workbench.action.debug.stepOver", "when": "debugState == 'stopped'" },
    { "key": "shift+f8", "command": "workbench.action.debug.stepOut", "when": "debugState == 'stopped'" },

    // 运行到光标 (IDEA: Alt+F9)
    { "key": "alt+f9", "command": "editor.debug.action.runToCursor", "when": "debugState == 'stopped'" },

    // 断点 (IDEA: Cmd+F8)
    { "key": "cmd+f8", "command": "editor.debug.action.toggleBreakpoint", "when": "debuggersAvailable && editorTextFocus" },

    // 计算表达式 (IDEA: Alt+F8)
    { "key": "alt+f8", "command": "editor.debug.action.selectionToRepl", "when": "debuggersAvailable && editorTextFocus && editorHasSelection" },

    // 构建 (IDEA: Cmd+F9)
    { "key": "cmd+f9", "command": "workbench.action.tasks.build" },
    { "key": "ctrl+r", "command": "workbench.action.tasks.reRunTask", "when": "taskCommandsRegistered && !terminalFocus && inDebugMode" },

    // ==================== Git ====================
    // 源代码管理视图 (Cmd+K)
    { "key": "cmd+k", "command": "workbench.view.scm" },

    // 提交 (IDEA: Cmd+K)
    {
        "key": "cmd+alt+k",
        "command": "git.pushTo",
        "when": "!inDebugMode && !terminalFocus"
    },

    // 拉取 (IDEA: Cmd+T)
    { "key": "cmd+t", "command": "git.sync" },

    // 查看变更 (IDEA: Cmd+D)
    { "key": "cmd+d", "command": "git.openChange", "when": "!editorTextFocus" },

    // 撤销变更 (IDEA: Cmd+Alt+Z)
    { "key": "cmd+alt+z", "command": "git.revertSelectedRanges", "when": "editorTextFocus && !editorReadonly" },

    // 下一个/上一个变更
    { "key": "shift+ctrl+alt+down", "command": "editor.action.dirtydiff.next", "when": "editorTextFocus" },
    { "key": "shift+ctrl+alt+up", "command": "editor.action.dirtydiff.previous", "when": "editorTextFocus" },

    // Diff 视图
    { "key": "f7", "command": "workbench.action.compareEditor.nextChange", "when": "textCompareEditorVisible" },
    { "key": "shift+f7", "command": "workbench.action.compareEditor.previousChange", "when": "textCompareEditorVisible" },

    // 合并冲突
    { "key": "shift+ctrl+right", "command": "merge-conflict.accept.current", "when": "isInDiffEditor" },
    { "key": "shift+ctrl+left", "command": "merge-conflict.accept.incoming", "when": "isInDiffEditor" },

    // ==================== 窗口/面板 ====================
    // 项目视图 (IDEA: Cmd+1)
    { "key": "cmd+1", "command": "workbench.action.toggleSidebarVisibility" },
    { "key": "cmd+1", "command": "workbench.action.toggleAuxiliaryBar", "when": "auxiliaryBarFocus" },
    { "key": "shift+escape", "command": "workbench.action.toggleSidebarVisibility", "when": "explorerViewletFocus" },

    // 切换第二个编辑器组 (Cmd+2)
    // { "key": "cmd+2", "command": "workbench.action.closeEditorsInOtherGroups" },

    // 终端 (IDEA: Alt+F12, 这里用 Cmd+3)
    { "key": "cmd+3", "command": "workbench.action.terminal.toggleTerminal", "when": "terminal.active" },
    { "key": "cmd+3", "command": "workbench.action.terminal.focus", "when": "terminalProcessSupported && !terminalFocus" },

    // 调试视图 (IDEA: Cmd+5)
    { "key": "cmd+5", "command": "workbench.view.debug" },
    { "key": "shift+cmd+f8", "command": "workbench.view.debug" },

    // 编辑器切换
    { "key": "shift+cmd+]", "command": "workbench.action.nextEditor" },
    { "key": "shift+cmd+[", "command": "workbench.action.previousEditor" },
    { "key": "ctrl+right", "command": "workbench.action.nextEditor" },
    { "key": "ctrl+left", "command": "workbench.action.previousEditor" },
    { "key": "ctrl+tab", "command": "workbench.action.quickOpenNavigateNext", "when": "inQuickOpen" },
    { "key": "alt+tab", "command": "workbench.action.focusNextGroup", "when": "editorFocus" },
    { "key": "shift+alt+tab", "command": "workbench.action.focusPreviousGroup", "when": "editorFocus" },

    // 移动标签页到其他编辑组
    { "key": "ctrl+cmd+left", "command": "workbench.action.moveEditorToLeftGroup" },
    { "key": "ctrl+cmd+right", "command": "workbench.action.moveEditorToRightGroup" },

    // 关闭编辑器
    { "key": "cmd+w", "command": "workbench.action.closeActiveEditor" },

    // 最大化编辑器 (IDEA: Cmd+Shift+F12)
    { "key": "shift+cmd+f12", "command": "workbench.action.maximizeEditor" },

    // 全屏
    { "key": "ctrl+cmd+f", "command": "workbench.action.toggleFullScreen" },

    // 新文件
    { "key": "cmd+n", "command": "workbench.action.files.newUntitledFile" },
    { "key": "shift+f4", "command": "workbench.action.files.showOpenedFileInNewWindow", "when": "emptyWorkspaceSupport" },

    // 保存全部 (IDEA: Cmd+S 保存全部)
    { "key": "cmd+s", "command": "workbench.action.files.saveAll" },

    // 复制文件路径
    { "key": "shift+cmd+c", "command": "copyFilePath", "when": "!editorFocus && !terminalFocus" },

    // ==================== Pin 标签 ====================
    { "key": "cmd+alt+p", "command": "workbench.action.pinEditor" },
    { "key": "cmd+alt+p", "command": "workbench.action.unpinEditor", "when": "activeEditorIsPinned" },

    // ==================== 命令面板/设置 ====================
    { "key": "shift+cmd+a", "command": "workbench.action.showCommands" },
    { "key": "shift+cmd+p", "command": "workbench.action.showCommands" },
    { "key": "cmd+,", "command": "workbench.action.openGlobalSettings" },
    { "key": "cmd+;", "command": "workbench.action.tasks.configureTaskRunner" },
    { "key": "ctrl+`", "command": "workbench.action.selectTheme" },
    { "key": "cmd+f1", "command": "workbench.action.showErrorsWarnings", "when": "editorTextFocus" },

    // ==================== 禁用冲突的默认快捷键 ====================
    { "key": "cmd+g", "command": "-editor.action.nextMatchFindAction", "when": "editorFocus" },
    { "key": "shift+cmd+d", "command": "-workbench.view.debug" },
    { "key": "ctrl+-", "command": "-workbench.action.navigateBack" },
    { "key": "ctrl+shift+-", "command": "-workbench.action.navigateForward" },
    { "key": "cmd+d", "command": "-editor.action.addSelectionToNextFindMatch" },
    { "key": "ctrl+g", "command": "-workbench.action.gotoLine" },
    { "key": "cmd+t", "command": "-workbench.action.showAllSymbols" },
    { "key": "shift+cmd+o", "command": "-workbench.action.gotoSymbol" },
    { "key": "cmd+b", "command": "-workbench.action.toggleSidebarVisibility" },
    { "key": "alt+cmd+f", "command": "-editor.action.startFindReplaceAction" },
    { "key": "shift+cmd+h", "command": "-workbench.action.replaceInFiles" },
    { "key": "f2", "command": "-editor.action.rename" },
    { "key": "alt+f8", "command": "-editor.action.marker.next" },
    { "key": "f5", "command": "-workbench.action.debug.continue" },
    { "key": "f9", "command": "-editor.debug.action.toggleBreakpoint" },
    { "key": "f10", "command": "-workbench.action.debug.stepOver" },
    { "key": "f11", "command": "-workbench.action.debug.stepInto" },
    { "key": "shift+f11", "command": "-workbench.action.debug.stepOut" },
    { "key": "ctrl+f5", "command": "-workbench.action.debug.run" },
    { "key": "shift+alt+f", "command": "-editor.action.formatDocument" },
    { "key": "shift+alt+o", "command": "-editor.action.organizeImports" },
    { "key": "alt+cmd+s", "command": "-workbench.action.files.saveAll" },
    { "key": "shift+cmd+k", "command": "-editor.action.deleteLines" },
    { "key": "shift+alt+down", "command": "-editor.action.copyLinesDownAction" },
    { "key": "alt+down", "command": "-editor.action.moveLinesDownAction" },
    { "key": "alt+up", "command": "-editor.action.moveLinesUpAction" },
    { "key": "ctrl+j", "command": "-editor.action.joinLines" },
    { "key": "shift+alt+a", "command": "-editor.action.blockComment" },
    { "key": "cmd+k cmd+0", "command": "-editor.foldAll" },
    { "key": "cmd+k cmd+j", "command": "-editor.unfoldAll" },
    { "key": "alt+cmd+[", "command": "-editor.fold" },
    { "key": "alt+cmd+]", "command": "-editor.unfold" },
    { "key": "shift+cmd+l", "command": "-editor.action.selectHighlights" },
    { "key": "cmd+u", "command": "-cursorUndo" },
    { "key": "cmd+k cmd+i", "command": "-editor.action.showHover" },
    { "key": "cmd+f12", "command": "-editor.action.goToImplementation" },
    { "key": "shift+alt+f12", "command": "-references-view.findReferences" },
    { "key": "shift+alt+h", "command": "-references-view.showCallHierarchy" },
    { "key": "cmd+k cmd+q", "command": "-workbench.action.navigateToLastEditLocation" },
    { "key": "cmd+k cmd+t", "command": "-workbench.action.selectTheme" },
    { "key": "cmd+k cmd+r", "command": "-git.revertSelectedRanges" },
    { "key": "alt+cmd+c", "command": "-copyFilePath" },
    { "key": "cmd+k o", "command": "-workbench.action.files.showOpenedFileInNewWindow" },
    { "key": "alt+cmd+left", "command": "-workbench.action.previousEditor" },
    { "key": "alt+cmd+right", "command": "-workbench.action.nextEditor" },
    { "key": "shift+cmd+space", "command": "-editor.action.triggerParameterHints" },
    { "key": "cmd+i", "command": "-editor.action.triggerSuggest" },
    { "key": "cmd+o", "command": "-workbench.action.files.openFile" },
    { "key": "cmd+o", "command": "-workbench.action.files.openFileFolder" },
    { "key": "cmd+.", "command": "-editor.action.quickFix" },
    {
        "key": "shift+cmd+b",
        "command": "-workbench.action.tasks.build"
    },
    {
        "key": "cmd+4",
        "command": "-workbench.action.focusFourthEditorGroup"
    },

    // ==================== 资源管理器 (Explorer) ====================
    // 新建文件
    { "key": "a", "command": "explorer.newFile", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

    // 新建文件夹
    { "key": "shift+a", "command": "explorer.newFolder", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

    // 重命名
    { "key": "r", "command": "renameFile", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

    // 删除
    { "key": "d", "command": "deleteFile", "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus && !editorTextFocus" },
    { "key": "shift+d", "command": "moveFileToTrash", "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus && !editorTextFocus" },

    // 复制文件
    { "key": "y", "command": "filesExplorer.copy", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

    // 剪切文件
    { "key": "x", "command": "filesExplorer.cut", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

    // 粘贴文件
    { "key": "p", "command": "filesExplorer.paste", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

    // 折叠文件夹
    { "key": "h", "command": "list.collapse", "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus && !editorTextFocus" },

